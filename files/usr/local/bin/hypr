#!/bin/sh

cd ~

# Log WLR errors and logs to the hyprland log. Recommended
export HYPRLAND_LOG_WLR=1

# DO NOT UNCOMMENT THIS!
##export SDL_VIDEODRIVER=wayland

#export GTK_THEME=Materia:light
