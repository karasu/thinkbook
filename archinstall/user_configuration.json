{
    "additional-repositories": [
        "multilib"
    ],
    "audio": "pipewire",
    "bootloader": "systemd-bootctl",
    "custom-commands": [
        "cd /home/karasu; git clone https://aur.archlinux.org/yay.git",
        "chown -R karasu:karasu /home/karasu/yay",
        "echo '\n[chaotic-aur]\nInclude = /etc/pacman.d/chaotic-mirrorlist' >> /etc/pacman.conf"

    ]
    "config_version": "2.5.6",
    "debug": false,
    "harddrives": [
        "/dev/nvme0n1"
    ],
    "swap": true,
    "hostname": "thinkbook",
    "kernels": [
        "linux"
    ],
    "keyboard-layout": "es",
    "mirror-region": "Worldwide",
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "offline": false,
    "packages": [
        "chromium"
    ],
    "script": "guided",
    "silent": false,
    "sys-encoding": "utf-8",
    "sys-language": "ca_ES.UTF-8",
    "timezone": "Europe/Rome",
    "version": "2.5.6"
}
